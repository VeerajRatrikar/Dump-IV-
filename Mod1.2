//Running Time Of An Algorithm
int runningTime(int arr_count, int* arr) {
    int count = 0;  
    int j;

    for (int i = 1; i < arr_count; i++) {
        int current_value = arr[i]; 

        for (j = i - 1; j >= 0; j--) {
            if (arr[j] > current_value) {
                arr[j + 1] = arr[j];
                count++;
            } else {
                break; 
            }
        }

        arr[j + 1] = current_value;
    }

    return count;  
}
